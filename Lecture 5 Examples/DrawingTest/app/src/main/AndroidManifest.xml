<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inika.drawingtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DrawingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="DrawGradientActivity"></activity>
        <activity
            android:name="DrawBitmapActivity"></activity>
        <activity
            android:name=".DrawShapeActivity"></activity>
        <activity
            android:name="DrawTextActivity"></activity>
        <activity
            android:name="DrawCustomFontActivity"></activity>

    </application>

</manifest>